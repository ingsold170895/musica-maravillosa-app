{
  "$schema": "./node_modules/@angular/cli/lib/config/schema.json",
  "version": 1,
  "newProjectRoot": "projects",
  "projects": {
    "electron-angular-musica-m": {
      "projectType": "application",
      "schematics": {},
      "root": "",
      "sourceRoot": "src",
      "prefix": "app",
      "architect": {
        "build": {
          "_comment": "\"builder\": \"@angular-guru/electron-builder:build\",",
          "builder": "@angular-devkit/build-angular:browser",
          "options": {
            "outputPath": "dist",
            "index": "src/index.html",
            "main": "src/main.ts",
            "polyfills": "src/polyfills.ts",
            "tsConfig": "tsconfig.app.json",
            "assets": [
              "src/favicon.ico",
              "src/assets",
              "src/worker.html",
              {
                "glob": "**/*",
                "input": "src/app/ngx-extended-pdf-viewer/assets/",
                "output": "/assets/"
              },
              {
                "glob": "**/*",
                "input": "src/app/ngx-extended-pdf-viewer/assets/",
                "output": "/assets/music/"
              }
            ],
            "scripts": [
              "rendered.js"
            ],
            "styles": [
              "src/styles.css",
              "src/theme.scss"
            ]
          },
          "configurations": {
            "production": {
              "fileReplacements": [
                {
                  "replace": "src/environments/environment.ts",
                  "with": "src/environments/environment.prod.ts"
                }
              ],
              "optimization": true,
              "outputHashing": "all",
              "sourceMap": false,
              "extractCss": true,
              "namedChunks": false,
              "aot": true,
              "extractLicenses": true,
              "vendorChunk": false,
              "buildOptimizer": true,
              "budgets": [
                {
                  "type": "initial",
                  "maximumWarning": "2mb",
                  "maximumError": "5mb"
                }
              ]
            }
          }
        },
        "serve": {
          "_comment": "\"builder\": @angular-guru/electron-builder:dev-server\"\"",
          "builder": "@angular-devkit/build-angular:dev-server",
          "options": {
            "browserTarget": "electron-angular-musica-m:build"
          },
          "configurations": {
            "production": {
              "browserTarget": "electron-angular-musica-m:build:production"
            }
          }
        },
        "extract-i18n": {
          "builder": "@angular-devkit/build-angular:extract-i18n",
          "options": {
            "browserTarget": "electron-angular-musica-m:build"
          }
        },
        "test": {
          "_comment": "\"builder\":@angular-guru/electron-builder:karma\"",
          "builder": "@angular-devkit/build-angular:karma ",
          "options": {
            "main": "src/test.ts",
            "polyfills": "src/polyfills.ts",
            "tsConfig": "tsconfig.spec.json",
            "karmaConfig": "karma.conf.js",
            "assets": [
              "src/favicon.ico",
              "src/assets",
              "src/worker.html",
              {
                "glob": "**/*",
                "input": "src/app/ngx-extended-pdf-viewer/assets/",
                "output": "/assets"
              },
              {
                "glob": "**/*",
                "input": "src/app/ngx-extended-pdf-viewer/assets/",
                "output": "/assets/music/"
              }
            ],
            "styles": [
              "src/styles.css",
              "src/theme.scss"
            ],
            "scripts": [
              "rendered.js"
            ]
          }
        },
        "lint": {
          "builder": "@angular-devkit/build-angular:tslint",
          "options": {
            "tsConfig": [
              "tsconfig.app.json",
              "tsconfig.spec.json",
              "e2e/tsconfig.json"
            ],
            "exclude": [
              "**/node_modules/**"
            ]
          }
        },
        "e2e": {
          "builder": "@angular-devkit/build-angular:protractor",
          "options": {
            "protractorConfig": "e2e/protractor.conf.js",
            "devServerTarget": "electron-angular-musica-m:serve"
          },
          "configurations": {
            "production": {
              "devServerTarget": "electron-angular-musica-m:serve:production"
            }
          }
        },
        "deploy": {
          "builder": "angular-cli-ghpages:deploy",
          "options": {}
        }
      }
    }
  },
  "defaultProject": "electron-angular-musica-m"
}